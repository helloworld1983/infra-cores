peripheral {
  name = "Control and status register for the MLVDS trigger";
  description = "Wishbone slave for control and status registers related to the MLVDS trigger";
  hdl_entity = "wb_trigger_iface_regs";

  prefix = "wb_trig_iface";

  ---------------------------------------------------------------------------
  -- CHANNEL 0
  ---------------------------------------------------------------------------

  reg {
    name = "Channel 0 Control";
    prefix = "ch0_ctl";

    field {
      name = "Trigger Direction";
      prefix = "dir";
      description = "1 - Receiver Mode\n0 - Transmitter Mode";
      type = BIT;
      --size = 1;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };

    field {
      name = "Trigger Direction Polarity";
      prefix = "dir_pol";
      description = "1 - Backplane trigger direction value will be reversed\n0 - Backplane Trigger direction value will be the same";
      type = BIT;
      --size = 1;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };

    field {
      name = "Receiver Pulse Counter Reset";
      prefix = "rcv_count_rst";
      description = "1 - Reset\n0 - Normal";
      type = MONOSTABLE;
      size = 1;
      clock = "fs_clk_i";
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };

    field {
      name = "Transmitter Pulse Counter Reset";
      prefix = "transm_count_rst";
      description = "1 - Reset\n0 - Normal";
      type = MONOSTABLE;
      size = 1;
      clock = "fs_clk_i";
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };

    field {
      name = "Trigger Polarity";
      prefix = "pol";
      description = "1 - Trigger polarity reversed (1 -> 0 pulse)\n0 - Trigger polarity direct (0 -> 1 pulse)";
      type = BIT;
      --size = 1;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };
  };

  reg {
    name = "Channel 0 Configuration Parameters";
    prefix = "ch0_cfg";

    field {
      name = "Receiver Pulse Length";
      prefix = "rcv_len";
      description = "Controls the length of the receiver deglitcher";
      type = SLV;
      size = 8;
      align = 0;
      clock = "fs_clk_i";
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };

    field {
      name = "Transmitter Pulse Length";
      prefix = "transm_len";
      description = "Controls the length of the transmitter output pulse";
      type = SLV;
      size = 8;
      align = 8;
      clock = "fs_clk_i";
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };
  };

  reg {
    name = "Channel 0 Transmitter/Receiver Pulses Counter";
    prefix = "ch0_count";

    field {
      name = "Receiver Counter";
      prefix = "rcv";
      description = "Counts the pulses received";
      type = SLV;
      size = 16;
      align = 0;
      clock = "wb_clk_i";
      access_bus = READ_ONLY;
      access_dev = WRITE_ONLY;
    };

    field {
      name = "Transmitter Counter";
      prefix = "transm";
      description = "Counts the pulses that will be transmitted";
      type = SLV;
      size = 16;
      align = 16;
      clock = "wb_clk_i";
      access_bus = READ_ONLY;
      access_dev = WRITE_ONLY;
    };
  };

  reg {
    name = "Channel 0 Transmitter Delay";
    prefix = "ch0_cfg_transm_delay_len";

    field {
      name = "Transmitter Delay Counter";
      description = "Delays a transmitter pulse by the number of clock cycles specified";
      type = SLV;
      size = 32;
      align = 0;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };
  };

  reg {
    name = "Channel 0 Receiver Delay";
    prefix = "ch0_cfg_rcv_delay_len";

    field {
      name = "Receiver Delay Counter";
      description = "Delays an incoming pulse by the number of clock cycles specified";
      type = SLV;
      size = 32;
      align = 0;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };
  };

  reg {
    name = "Channel 0 Transmitter Pulse Train Generator";
    prefix = "ch0_cfg_transm_pulse_train";

    field {
      name = "Number of output pulses";
      description = "Specifies how many output pulses per input pulses";
      prefix = "num";
      type = SLV;
      size = 16;
      align = 0;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };

    field {
      name = "Reserved";
      description = "Ignore on read, write with 0's";
      prefix = "reserved";
      type = SLV;
      size = 16;
      access_bus = READ_WRITE;
      access_dev = READ_ONLY;
    };
  };

